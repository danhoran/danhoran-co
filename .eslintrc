{
    "extends": "airbnb",
    "rules":{
        "indent": [2, 4, { "SwitchCase": 1 }],
        "react/jsx-indent": [2, 4],
        "react/jsx-indent-props": [2, 4],
        "react/jsx-filename-extension": [0, { "extensions": [".js"] }]
    },
    "env": {
        "browser": true,
        "node": true,
        "jest": true
    }
}
